WIN64_RUNNING=$(if $(WINDIR),$(if $(wildcard $(WINDIR)\SysWOW64),1,),)
WIN64_FLAGS=-m64 -mwin32
WIN64_LFLAGS=$(WIN64_FLAGS) -mdll
WIN64_EFLAGS=$(WIN64_FLAGS) -mwindows
WIN64_LIB_PFX=
WIN64_LIB_EXT=.dll
WIN64_LIB_SFX=64.dll
WIN64_APP_PFX=
WIN64_APP_EXT=.exe
WIN64_APP_SFX=64.exe
WIN64_OBJ_PFX=
WIN64_OBJ_EXT=.obj
WIN64_OBJ_SFX=.win64.obj
WIN64_ARC_PFX=
WIN64_ARC_EXT=.lib
WIN64_ARC_SFX=64.lib